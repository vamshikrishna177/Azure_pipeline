{
	"name": "transformCSV",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Dataflowcsv",
						"type": "DatasetReference"
					},
					"name": "sourceCSV"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "dataflow",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "rowsfilter"
				},
				{
					"name": "split1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "aggregate1"
				},
				{
					"name": "alterRow1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          country as string,",
				"          iso3 as string,",
				"          year as short,",
				"          sex as string,",
				"          alcohol_liters_per_capita as double,",
				"          lower_ci as double,",
				"          upper_ci as double,",
				"          ci_width as double,",
				"          IndicatorCode as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourceCSV",
				"sourceCSV select(mapColumn(",
				"          country,",
				"          iso3,",
				"          year,",
				"          sex,",
				"          alcohol_liters_per_capita,",
				"          IndicatorCode",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 filter(year != 2003) ~> rowsfilter",
				"rowsfilter split(year <=2010,",
				"     disjoint: false) ~> split1@(before2010, after2010)",
				"split1@after2010 derive(alcohol_liters_per_capita = round(alcohol_liters_per_capita,4)) ~> derivedColumn1",
				"split1@before2010 aggregate(groupBy(country,",
				"          year),",
				"     alcohol_liters_per_capita = max(alcohol_liters_per_capita)) ~> aggregate1",
				"aggregate1 alterRow(insertIf(1==1)) ~> alterRow1",
				"alterRow1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}